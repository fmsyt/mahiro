  {
    "id": "volume",
    "type": "command",
    "style": "slider",
    "sync": true,
    "commands": [
      "nircmd.exe",
      "setsysvolume",
      "{context}"
    ],
    "props": {
      "min": 0,
      "max": 65535,
      "step": 655
    },
    "platform": "Windows"
  },
  {
    "id": "volume_pa",
    "type": "command",
    "commands": ["pacmd", "set-sink-volume", "@DEFAULT_SINK@", "{context}"],
    "style": "slider",
    "default": {
      "command": "pacmd list-sinks | grep -A 22 '* index' | grep 'volume: front' | awk '{print $3}'",
      "value": 0
    },
    "props": {
      "min": 0,
      "max": 65535,
      "step": 655
    }
  }

]
